{
  "name": "@rescript/runtime",
  "version": "12.0.0-alpha.13",
  "description": "ReScript runtime modules",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescript-lang/rescript.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/es6",
    "lib/js",
    "*.res",
    "*.resi"
  ],
  "exports": {
    "./lib/es6/*": "./lib/es6/*",
    "./lib/js/*": "./lib/js/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:rewatch": "rewatch build",
    "build:bsb": "rescript build",
    "clean": "rewatch clean"
  },
  "devDependencies": {
    "rescript": "workspace:^"
  },
  "preferUnplugged": true
}
